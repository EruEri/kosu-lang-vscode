{
  "name": "kosu-language",
  "displayName": "Kosu Language",
  "description": "Kosu programming language support",
  "license": "MIT",
  "version": "0.2.1",
  "repository": {
    "url": "https://github.com/EruEri/kosu-lang-vscode"
  },
  "bugs": {
    "url": "https://github.com/EruEri/kosu-lang-vscode/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "kosu",
      "aliases": ["Kosu", "kosu"],
      "extensions": [".kosu"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "kosu",
      "scopeName": "source.kosu",
      "path": "./syntaxes/kosu.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "kosu",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
